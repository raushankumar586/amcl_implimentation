<package format="2">

    <name>path_planner</name>
    <version>0.0.1</version>
    <description>ROS node for A* path planning</description>
    <maintainer email="liyang.liu@uts.edu.au">Liyang Liu</maintainer>
    <license>BSD-3-Clause</license>

    <buildtool_depend>catkin</buildtool_depend>

